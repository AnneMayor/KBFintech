<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.finance.mapper.KAccountMapper">

	<select id="getAccountMoney" parameterType="String" resultType="int">
		SELECT ACCOUNT_MONEY FROM K_ACCOUNT WHERE ACCOUNT_NUMBER = #{ACCOUNT_NUMBER}
	</select>
	
	<select id="isRegister" parameterType="String" resultType="String">
		SELECT ACCOUNT_NUMBER FROM K_ACCOUNT WHERE PHONE = #{phone}
	</select>
	
</mapper>